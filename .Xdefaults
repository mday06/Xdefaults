! $OpenBSD: dot.Xdefaults,v 1.3 2014/07/10 10:22:59 jasper Exp $
XTerm*loginShell:true
URxvt*loginShell:true
URxvt*geometry: 80x-1
URxvt*depth: 32
!URxvt*background: [80]#24273A

! special
*.cursorColor:  #ebdbb2
!*background: #24273A
!*foreground: #CAD3F5
!
!! black
!*color0: #494D64
!*color8: #5B6078
!!
!! red
!*color1: #ED8796
!*color9: #ED8796
!
!! green
!*color2: #A6DA95
!*color10: #A6DA95
!
!! yellow
!*color3: #EED49F
!*color11: #EED49F
!
!! blue
!*color4: #8AADF4
!*color12: #8AADF4
!
!! magenta
!*color5: #F5BDE6
!*color13: #F5BDE6
!
!! cyan
!*color6: #8BD5CA
!*color14: #8BD5CA
!
!! white
!*color7: #B8C0E0
!*color15: #A5ADCB

! hard contrast: 
! *background: #1d2021
! URxvt*background: #282828
! soft contrast: 
*background: [80]#32302f
*foreground: #ebdbb2
! Black + DarkGrey
*color0:  #282828
*color8:  #928374

! DarkRed + Red
*color1:  #cc241d
*color9:  #fb4934

! DarkGreen + Green
*color2:  #98971a
*color10: #b8bb26

! DarkYellow + Yellow
*color3:  #d79921
*color11: #fabd2f

! DarkBlue + Blue
*color4:  #458588
*color12: #83a598

! DarkMagenta + Magenta
*color5:  #b16286
*color13: #d3869b

! DarkCyan + Cyan
*color6:  #689d6a
*color14: #8ec07c

! LightGrey + White
*color7:  #a89984
*color15: #ebdbb2

XTerm*cursorBlink: True
XTerm*font: xft:JetBrains Mono:style=Regular:pixelsize=22
XTerm*boldFont: xft:JetBrains Mono:style=Bold:pixelsize=22
XTerm*italicFont: xft:JetBrains Mono:style=Italic:pixelsize=22
XTerm*boldItalicFont: xft:JetBrains Mono:style=Bold Italic:pixelsize=22
XTerm*scrollBar: False
XTerm*scrollBar_right: False
Xft.rgba: rgb

URxvt*cursorBlink: True
URxvt*font: xft:JetBrains Mono:style=Regular:pixelsize=22
URxvt*boldFont: xft:JetBrains Mono:style=Bold:pixelsize=22
URxvt*italicFont: xft:JetBrains Mono:style=Italic:pixelsize=22
URxvt*boldItalicFont: xft:JetBrains Mono:style=Bold Italic:pixelsize=22
URxvt*scrollBar: False
URxvt*scrollBar_right: False
